{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "services/auth/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "services/auth/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "services/auth",
  "projects": {
    "auth": {
      "type": "application",
      "root": "services/auth",
      "entryFile": "main",
      "sourceRoot": "services/auth/src",
      "compilerOptions": {
        "tsConfigPath": "services/auth/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "ingestion": {
      "type": "application",
      "root": "services/ingestion",
      "entryFile": "main",
      "sourceRoot": "services/ingestion/src",
      "compilerOptions": {
        "tsConfigPath": "services/ingestion/tsconfig.app.json"
      }
    },
    "responder": {
      "type": "application",
      "root": "services/responder",
      "entryFile": "main",
      "sourceRoot": "services/responder/src",
      "compilerOptions": {
        "tsConfigPath": "services/responder/tsconfig.app.json"
      }
    },
    "sync": {
      "type": "application",
      "root": "services/sync",
      "entryFile": "main",
      "sourceRoot": "services/sync/src",
      "compilerOptions": {
        "tsConfigPath": "services/sync/tsconfig.app.json"
      }
    }
  }
}
